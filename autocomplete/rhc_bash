          opts="--app --application-id --certificate --namespace --passphrase --private-key"
        else
          opts=""
        fi
        ;;

    esac
    IFS=$SAVE_IFS
  fi

  COMPREPLY=( $(compgen -W "${opts}" -- ${cur}) )
  return 0
}

