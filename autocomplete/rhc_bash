          opts=""
        fi
        ;;

      "app cartridge-add")
        if [[ "$cur" == -* ]]; then
          opts="--app --application-id --cartridge --env --namespace"
        else
          opts=""
        fi
        ;;

      "app cartridge-list")
        if [[ "$cur" == -* ]]; then
          opts="--verbose"
        else
          opts=""
        fi
        ;;

      "app cartridge-reload")
        if [[ "$cur" == -* ]]; then
          opts="--app --application-id --cartridge --namespace"
        else
          opts=""
        fi
        ;;

      "app cartridge-remove")
        if [[ "$cur" == -* ]]; then
          opts="--app --application-id --cartridge --confirm --namespace"
        else
          opts=""
        fi
        ;;

      "app cartridge-restart")
        if [[ "$cur" == -* ]]; then
          opts="--app --application-id --cartridge --namespace"
        else
          opts=""
        fi
        ;;

      "app cartridge-scale")
        if [[ "$cur" == -* ]]; then
