          opts=""
        else
          opts=""
        fi
        ;;

      "app authorization-list")
        if [[ "$cur" == -* ]]; then
          opts=""
        else
          opts=""
        fi
        ;;

      "app authorizations")
        if [[ "$cur" == -* ]]; then
          opts=""
        else
          opts=""
        fi
        ;;

      "app cartridge")
        if [[ "$cur" == -* ]]; then
          opts=""
        else
          opts="list add show remove start stop restart status reload scale storage"
        fi
        ;;

